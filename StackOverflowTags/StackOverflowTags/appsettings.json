{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Information",
      "Microsoft": "Error"
    }
  },
  "AllowedHosts": "*",
  "EndpointHosts": {
    "StackOverflow": {
      "Tags": "http://api.stackexchange.com/2.3/tags?page={0}&pagesize={1}&order=desc&sort=popular&site=stackoverflow"
    }
  },
  "Application": {
    "TagsJsonField": "items"
  }
}
